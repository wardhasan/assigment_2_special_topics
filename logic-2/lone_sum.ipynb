    "def lone_sum(a, b, c):\n",
    "  if a != b and a !=c and b!=c:\n",
    "    return a+b+c\n",
    "  elif  a == b and a != c:\n",
    "    return c\n",
    "  elif b == c and b != a:\n",
    "    return a\n",
    "  elif a == c and a !=b:\n",
    "    return b\n",
    "  elif a == b and b==c:\n",
    "    return 0\n",
    "  \n"
