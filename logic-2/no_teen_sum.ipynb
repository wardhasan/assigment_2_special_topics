    "def no_teen_sum(a, b, c):\n",
    "  a1 = fix_teen(a)\n",
    "  b1 = fix_teen(b)\n",
    "  c1 = fix_teen(c)\n",
    "  return a1+b1+c1\n",
    "def fix_teen(n):\n",
    "  if n == 15 or n == 16:\n",
    "    return n\n",
    "  elif n >=13 and n<15: \n",
    "    return 0\n",
    "  elif n>16 and n<=19:\n",
    "    return 0\n",
    "  else:\n",
    "    return n\n"
