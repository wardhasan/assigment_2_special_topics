    "def count_evens(nums):\n",
    "  int_nums = 0\n",
    "  for i in range(len(nums)):\n",
    "    if nums[i] % 2 == 0:\n",
    "      int_nums+=1\n",
    "    \n",
    "  return int_nums  \n"
