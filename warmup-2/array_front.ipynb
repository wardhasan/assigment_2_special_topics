    "  if len(nums) > 4:\n",
    "    newsize = 4\n",
    "    if 9 in nums[:newsize]:\n",
    "      return True\n",
    "    else:\n",
    "      return False\n",
    "  elif len(nums) <= 4 :\n",
    "    if 9 in nums:\n",
    "      return True\n",
    "    else:\n",
    "      return False"
